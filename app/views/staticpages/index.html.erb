<div class="container">
	<div class="row">
		<div class="col-xs-12 col-md-7" style="border:1px solid blue">.col-md-1</div>
		<div class="col-xs-12 col-md-3 col-md-offset-1" style="border:1px solid blue">


<table>
  <thead>
    <tr>
      <th>Published</th>
      <th>Title</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.published %></td>
        <td><%= event.title %></td>
        <td><%= event.text %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>



		</div>
	</div>
</div>
